datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model FiveLetterWord {
  id        Int               @id @default(autoincrement())
  word      String            @unique
  games     FiveLetterGame[]
}

model FiveLetterGame {
  id        Int             @id @default(autoincrement())
  word      FiveLetterWord  @relation(fields: [wordId], references: [id])
  wordId    Int
}
